{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-alert": "off",
        "no-underscore-dangle": "off",
        "no-restricted-globals": "off",
        "linebreak-style": 0,
        "consistent-return": 0
    }
}
